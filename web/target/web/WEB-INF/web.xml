<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!--指定欢迎界面-->
  <welcome-file-list>
    <welcome-file>/WebContent/common/hello.html</welcome-file>
    <welcome-file>/WebContent/common/hello.jsp</welcome-file>
  </welcome-file-list>
  <!--指定错误界面-->
  <error-page>
    <error-code>404</error-code>
    <location>/WebContent/common/404.jsp</location>
  </error-page>
</web-app>
